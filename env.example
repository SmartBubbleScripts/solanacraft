# Solana Network Configuration
NEXT_PUBLIC_SOLANA_NETWORK=mainnet

# Commission Wallet (where fees are sent)
NEXT_PUBLIC_COMMISSION_WALLET_ADDRESS=YOUR_COMMISSION_WALLET_ADDRESS

# RPC Endpoints (in order of priority)
# Primary: QuickNode (free tier - 3M requests/month)
NEXT_PUBLIC_QUICKNODE_RPC_URL=https://multi-withered-sky.solana-mainnet.quiknode.pro/YOUR_QUICKNODE_API_KEY/
# Fallback 1: Helius (free tier - 100M requests/month)
NEXT_PUBLIC_HELIUS_RPC_URL=https://rpc.helius.xyz/?api-key=YOUR_HELIUS_API_KEY
# Fallback 2: Alchemy (free tier - 300M compute units/month)
NEXT_PUBLIC_ALCHEMY_RPC_URL=https://solana-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY

# Production Settings
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=production
MAX_REQUESTS_PER_MINUTE=1000
RPC_RETRY_ATTEMPTS=3
RPC_RETRY_DELAY=1000
